code = "shader_type canvas_item;

void fragment() {
	vec3 c = textureLod(SCREEN_TEXTURE, SCREEN_UV, 0.0).rgb;

	//c.b += 0.2;
	//c.g += 0.2;
	//c.r += 0.2;
	float y = SCREEN_UV.y - 0.12;
	float blue = clamp(c.b * (1.0- y * y * y * y * y * y * y * y ), 0.23, 1);
	float green = clamp(c.g * (1.0- y * y * y * y * y * y), 0.14, 1);
	float red = clamp(c.r * (1.0- y * y  * y * y * y * y), 0.20, 1);
	
	blue = round(blue * 60.0) / 60.0;
	green = round(green * 60.0) / 60.0;
	red = round(red * 60.0) / 60.0;
	
	c.b = blue;
	c.g = green;
	c.r = red;

	COLOR.rgb = c;
}
"

