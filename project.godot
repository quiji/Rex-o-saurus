[autoload]

Resolution="*res://classes/Resolution.gd"

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "quijipixel.notes", "quijipixel.todo" )

[rendering]

environment/default_environment="res://default_env.tres"
